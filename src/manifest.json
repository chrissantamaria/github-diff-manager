{
  "name": "GitHub Diff Manager",
  "manifest_version": 2,
  "description": "Tool to help manage PR review",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false,
    "chrome_style": true
  },
  "permissions": ["storage", "webNavigation", "https://github.com/*"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
